<head>
  <script>
    function empty() {
    var input_category = document.getElementById("category-num").value;
    if (input_category == "") {
      alert("카테고리를 선택해주세요.");
      return false;
    };
    
    var input_post_type = $('#type-vs').val();
    
    var input_first_label = document.getElementById("first_choice_label").value;
    if(input_first_label == "" && input_post_type=="vs"){
      alert("투표항목1을 입력해주세요.");
      return false;
    };
    
    var input_second_label = document.getElementById("second_choice_label").value;
    if(input_second_label == "" && input_post_type=="vs"){
      alert("투표항목2를 입력해주세요.");
      return false;
    };
    
    var input_vstitle = document.getElementById("vs-title").value;
    if(input_vstitle == "" && input_post_type=="vs"){
      alert("제목을 입력해주세요.");
      return false;
    };
    
    var input_freetitle = document.getElementById("free-title").value;
    if(input_freetitle == "" && input_post_type=="free"){
      alert("제목을 입력해주세요.");
      return false;
    };
  
    
  }
  
  function inputLengthCheck(eventInput){
    var inputText = $(eventInput).val();
    var inputMaxLength = $(eventInput).prop("maxlength");
    var j = 0;
    var count = 0;
    for(var i = 0;i < inputText.length;i++) { 
      val = escape(inputText.charAt(i)).length; 
      if(val == 6){
        j++;
      }
      
      j++;
      
      if(j <= inputMaxLength){
        count++;
      }
    }

  if(j > inputMaxLength){
  $(eventInput).val(inputText.substr(0, count));
  }
}


  </script>
