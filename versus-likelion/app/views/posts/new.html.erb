<script>
  $('#switch label').on('click', function(){
  var indicator = $(this).parent('.switch').find('span');
  if ( $(this).hasClass('right') ){
		$(indicator).addClass('right');
		$("#speaker-on").val("off").trigger('change');
		$("#speaker-off").val("off").trigger('change');
		
  } else {
    $(indicator).removeClass('right');
    $("#speaker-on").val("on").trigger('change');
		$("#speaker-off").val("on").trigger('change');
  }
});
$('.switch.vs label').on('click', function(){
  var indicator = $(this).parent('.switch').find('span');
  if ( $(this).hasClass('right') ){
		$(indicator).addClass('right');
		$("#type-vs").val("free").trigger('change');
		$("#type-free").val("free").trigger('change');
		$(".choice-vs").css("display", "none");
		$("#vs-title").attr("name", "");
		$("#free-title").attr("name","title");
		$(".choice-free").css("display", "block");
		
  } else {
    $(indicator).removeClass('right');
    $(".choice-vs").css("display", "block");
    $("#type-vs").val("vs").trigger('change');
		$("#type-free").val("vs").trigger('change');
    $("#vs-title").attr("name", "title");
    $("#free-title").attr("name","");
		$(".choice-free").css("display", "none");
  }
});


</script>

